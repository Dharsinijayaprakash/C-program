//perfect number
#include <stdio.h>
int main()
{
    int g,e,sum=0;
    scanf("%d",&g);
    for(e=1;e<g;e++)
    {
    if(g%e==0)
    {
       sum=sum+e;    
       printf("%d",e); 
    }
    }
    if(sum==g)
    {
        printf(" %d is a perfect number",g);
    }
    else
    {
        printf(" %d is not a perfect number",g);
    }
    return 0;
}
